        FontFamily="{materialDesign:MaterialDesignFont}"
        xmlns:local="clr-namespace:Yacht_Connector"
        mc:Ignorable="d"
        Title="_3DTestPage" Height="720" Width="500" ResizeMode="CanMinimize">

    <Grid>
        <local:DiceRoller Margin="5,10,5,89" x:Name="roller" />
        <Button Name="b" Margin="0,636,0,23" Click="b_Click" Height="80"/>
    </Grid>

</Window>
